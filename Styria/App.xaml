<Application x:Class="Styria.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Styria"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="mainImage" UriSource="Images\main_image.jpg"></BitmapImage>
            <BitmapImage x:Key="emptyTab" UriSource="Images\empty_tab.png"></BitmapImage>
        </ResourceDictionary>
    </Application.Resources>
</Application>